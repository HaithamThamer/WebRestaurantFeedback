    <style>
        * {
            margin: 0px;
            padding: 0px;
        }

        html,
        body {
            height: 100%;
            margin: 0px;
            overflow: hidden;
        }

        ul {
            list-style-type: none;
            align-content: center;
            align-items: center;
            align-self: center;
            text-align: center;
            height: 100%;
            display: block;
            position: relative;

        }

        li {
            display: inline-block;
